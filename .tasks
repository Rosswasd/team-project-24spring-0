# vim: set fenc=utf-8 ft=taskini:
# see: https://github.com/skywind3000/asynctasks.vim/wiki/Task-Config

[project-init]
command=cargo update
cwd=<root>/src-tauri

[project-build]
command=cargo build
cwd=<root>/src-tauri
errorformat=%. %#--> %f:%l:%c

[project-run]
command=cargo test
cwd=<root>/src-tauri
output=terminal

